.top-header{
  display flex
  justify-content space-between
  position fixed
  width 100%
  box-sizing border-box
  border 1px solid #e3e3e3
  align-items center
  padding 0 20px
  background-color #e3e3e3
  box-shadow 0 0 5px steelblue
  .right-content{
    ul{
      margin 0
      padding 0
      li{
        display inline-block
        margin-right 20px
        line-height 50px
        cursor pointer
      }
    }
  }
}